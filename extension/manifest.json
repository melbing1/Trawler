{
	"browser_action": {
		"browser_style": true,
		"default_title": "Trawler",
		"default_popup": "troubleshooting.html"
	},
	"manifest_version": 2,
	"name": "Trawler",
	"version": "1.0",
	"description": "An antiphishing firefox extension",
	"main": "main.js",
	"content_scripts": [
		{
		"matches": ["<all_urls>"],
		"js": ["content_scripts/content_script.js"]
		}
	],
	"permissions": [
		"<all_urls>"
	]
}
